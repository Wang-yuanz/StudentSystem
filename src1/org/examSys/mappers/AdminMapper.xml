<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.examSys.dao.AdminDao">
	<select id="login" resultType="org.examSys.pojo.Admin">
		select * from admin where loginId = #{loginId} and loginPwd = #{loginPwd}
	</select>
</mapper>  